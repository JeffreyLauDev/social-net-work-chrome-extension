{
  "manifest_version": 2,

  "name": "Soical Net Work",
  "description": "Quick way to check Tlist",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://www.instagram.com/*"]
  }]
}
